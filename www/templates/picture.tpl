<div class="picture-item" picId="{{id}}">
    <img src="/pictures/{{filename}}" />
    <b>Title: {{title}}</b>
    <p>Description: {{description}}</p>
    <i>Place: {{place}}</i>
    <section class="tool-buttons">
        <span class="tb-delete"></span>
        <span class="tb-edit"></span>
        <span class="tb-download"></span>
    </section>
</div>